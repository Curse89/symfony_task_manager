<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('assets/view/bootstrap/css/bootstrap.min.css') }}">
            <link rel="stylesheet" href="{{ asset('assets/view/css/main.css') }}">
        {% endblock %}
    </head>
    <header>
        {{ include('public/_header.html.twig') }}
    </header>
    <body>
        {% block body %}{% endblock %}

        {% block javascripts %}
            <script src="{{ asset('assets/view/bootstrap/js/bootstrap.bundle.js') }}"></script>
            <script src="{{ asset('assets/view/js/main.js') }}"></script>
        {% endblock %}
    </body>
</html>
